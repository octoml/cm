{
  "alias": "prototype-install-tvm",
  "automation_alias": "script",
  "automation_uid": "5b4e0237da074764",
  "default_version": "1",
  "deps": [
    {
      "tags": "set,echo-off,win"
    },
    {
      "tags": "get,llvm"
    }
  ],
  "install": true,
  "post_deps": [
    {
      "take_version_from_env": true
    }
  ],
  "tags": [
    "prototype",
    "install",
    "tvm",
    "get-tvm"
  ],
  "uid": "93c89140e6224f4b",
  "versions": {
    "dev": {
      "env": {
        "PACKAGE_GIT_CHECKOUT": "main"
      }
    },
    "dev-dnnl-int8": {
      "env": {
        "PACKAGE_GIT_URL": "https://github.com/apeskov/tvm",
        "PACKAGE_GIT_CHECKOUT": "ap/dnnl-int8"
      }
    }
  },
  "variations": {
    "llvm": {
      "env": {
	"CM_TVM_USE_LLVM": "yes"
      }
    },
    "openmp": {
      "env": {
	"CM_TVM_USE_OPENMP": "yes"
      }
    }
  }
}
